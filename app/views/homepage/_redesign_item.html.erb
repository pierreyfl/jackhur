	.gig-wrapper .media .slider {
	    position: absolute;
	    top: 0;
	    left: 0;
	    width: 100%;
	    height: 100%;
	    z-index: 1;
	}

	.gig-wrapper .media .slider .slides {
	    height: 100%;
